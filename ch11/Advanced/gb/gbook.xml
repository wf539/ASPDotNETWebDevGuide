<gbook>
  <xsd:schema id="gbook" targetNamespace="" xmlns="" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
    <xsd:element name="gbook" msdata:IsDataSet="true">
      <xsd:complexType>
        <xsd:choice maxOccurs="unbounded">
          <xsd:element name="gbooky">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="Name" type="xsd:string" minOccurs="0" />
                <xsd:element name="Emoticon" type="xsd:string" minOccurs="0" />
                <xsd:element name="Email" type="xsd:string" minOccurs="0" />
                <xsd:element name="Comments" type="xsd:string" minOccurs="0" />
                <xsd:element name="Chrono" type="xsd:string" minOccurs="0" />
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
    </xsd:element>
  </xsd:schema>
</gbook>